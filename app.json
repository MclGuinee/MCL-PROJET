{
  "expo": {
    "name": "MCL",
    "description": "MCL",
    "slug": "MCL",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mcl.ios",
      "config": {
        "googleMapsApiKey": process.env.REACT_APP_GOOGLE_MAP_API_KEY_IOS
      }
    },
    "android": {
      "package": "com.mcl.android",
      "config": {
        "googleMaps": {
          "apiKey": process.env.REACT_APP_GOOGLE_MAP_API_KEY_ANDROID
        }
      }
    }
  }
}